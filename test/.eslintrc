{
  "extends": "airbnb/legacy",
  "env": {
    "browser": false,
    "jasmine": true,
    "node": true
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "func-names": 0,
    "id-length": 0,
    "max-len": 0,
    "no-param-reassign": [2, {"props": false}],
    "no-unused-vars": [2, {"args": "none"}],
    "no-use-before-define": [2, "nofunc"],
    "object-curly-spacing": [2, "never"],
    "padded-blocks": 0,
    "strict": [2, "global"],
    "vars-on-top": 0
  },
  "globals": {
    "angular": 1
  }
}
